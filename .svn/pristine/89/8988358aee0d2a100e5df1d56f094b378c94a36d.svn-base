    opacity: 0.8;
    filter: alpha(opacity=80);
    cursor: pointer;
    color: #fff;
    line-height: 80px;
    font-size: 30px;
    text-align: center;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

.left_btn {
    float: left;
}

.right_btn {
    float: right;
}
