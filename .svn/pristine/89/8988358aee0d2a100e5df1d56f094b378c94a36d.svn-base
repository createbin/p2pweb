        .risking_box ul li p {
            padding-left: 10px;
            height: 30px;
            line-height: 30px;
            position: relative;
        }

            .risking_box ul li p em {
                position: absolute;
                right: 10px;
                top: 0px;
                color: #ff6666;
            }

                .risking_box ul li p em:before {
                    content: "";
                    display: inline-block;
                    width: 19px;
                    height: 19px;
                    background: url(../images/risking_warn.png);
                    vertical-align: -4px;
                    margin-right: 5px;
                }

        .risking_box ul li span {
            margin-right: 55px;
            margin-left: 10px;
        }

            .risking_box ul li span input[type="radio"] + label::before {
                content: "\a0"; /*不换行空格*/
                display: inline-block;
                vertical-align: middle;
                font-size: 14px;
                width: 1em;
                height: 1em;
                margin-right: .4em;
                border-radius: 50%;
                border: 1px solid #cecece;
                text-indent: .15em;
                line-height: 1;
            }

            .risking_box ul li span input[type="radio"]:checked + label::before {
                background-color: #1a9bfc;
                background-clip: content-box;
                padding: .2em;
                width: 0.6em;
                height: 0.6em;
                border: 1px solid #1a9bfc;
            }

            .risking_box ul li span input[type="radio"] {
                position: absolute;
                clip: rect(0, 0, 0, 0);
            }

